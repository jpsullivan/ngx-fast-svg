    "angular-svg-icon": "^13.0.0",
    "rxjs": "^7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.6",
