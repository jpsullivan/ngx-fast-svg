{
  "name": "ngx-fast-svg",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "npm run serve",
    "start:ssr": "npm run serve:ssr",
    "serve": "nx serve ngx-fast-icon-demo",
    "serve:prod": "npm run serve -- --configuration=production",
    "serve:ssr": "nx serve-ssr ngx-fast-icon-demo",
    "serve:ssr:prod": "npm run serve:ssr -- --configuration=production",
    "build": "nx build ngx-fast-icon-demo",
    "build-demo-to-docs": "nx run ngx-fast-icon-demo:prerender:gh-pages && npm run create-404",
    "build-demo": "nx build ngx-fast-icon-demo --configuration=development",
    "build:prod": "npm run build -- --configuration production",
    "build:prod:stats": "npm run build -- --configuration production --stats-json",
    "build-server": "nx build-server ngx-fast-icon-demo",
    "build:prod:ssr": "npm run build:prod && npm run build-server",
    "postinstall": "ngcc --properties es2020 browser module main",
    "user-flow:collect": "npx @push-based/user-flow -p ./packages/ngx-fast-icon-demo/perf/.user-flowrc.json collect -v",
    "update-bundle-state:readme": "npm run build:prod:stats && npx ts-node -P tools/tsconfig.tools.json  ./tools/bundle-stats/index.ts update-bundle-stats --stats=./dist/packages/browser/ngx-fast-icon-demo/stats.json --target=./README.md",
    "update-svg-size:readme": "npm run build:prod:stats && npx ts-node -P tools/tsconfig.tools.json  ./tools/bundle-stats/index.ts update-svg-size --folder=./packages/ngx-fast-icon-demo/src/assets/svg-icons --target=./README.md",
    "create-404": "node -e \"require('fs').copyFile('docs/index.html', 'docs/404.html', function(err) { if (err) console.log(err); console.log('File successfully renamed!') })\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.0.4",
    "@angular/cdk": "15.0.4",
    "@angular/common": "15.0.4",
    "@angular/compiler": "15.0.4",
    "@angular/core": "15.0.4",
    "@angular/forms": "15.0.4",
    "@angular/material": "15.0.4",
    "@angular/platform-browser": "15.0.4",
    "@angular/platform-browser-dynamic": "15.0.4",
    "@angular/platform-server": "15.0.4",
    "@angular/router": "15.0.4",
    "@ant-design/icons-angular": "^15.0.0",
    "@fortawesome/angular-fontawesome": "^0.10.2",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@ionic/angular": "^6.1.2",
    "@nguniversal/express-engine": "15.0.0",
    "angular-svg-icon": "^13.0.0",
    "rxjs": "^7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.0.5",
    "@angular-eslint/eslint-plugin": "15.0.0",
    "@angular-eslint/eslint-plugin-template": "15.0.0",
    "@angular-eslint/template-parser": "15.0.0",
    "@angular/cli": "~15.0.0",
    "@angular/compiler-cli": "15.0.4",
    "@angular/language-service": "15.0.4",
    "@jscutlery/semver": "^2.29.3",
    "@nguniversal/builders": "15.0.0",
    "@nrwl/angular": "15.4.5",
    "@nrwl/cli": "15.4.5",
    "@nrwl/cypress": "15.4.5",
    "@nrwl/eslint-plugin-nx": "15.4.5",
    "@nrwl/jest": "15.4.5",
    "@nrwl/linter": "15.4.5",
    "@nrwl/nx-cloud": "15.0.2",
    "@nrwl/workspace": "15.4.5",
    "@nrwl/devkit": "15.4.5",
    "@push-based/user-flow": "^0.7.5",
    "@types/compression": "^1.7.2",
    "@types/jest": "28.1.8",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "compression": "^1.7.4",
    "cypress": "^9.5.4",
    "domino": "^2.1.6",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.12.1",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.1",
    "jest-preset-angular": "12.2.3",
    "ng-packagr": "15.0.3",
    "nx": "15.4.5",
    "postcss-preset-env": "~7.5.0",
    "prettier": "2.6.2",
    "ts-jest": "28.0.8",
    "typescript": "4.8.4"
  },
  "workspaces": [
    "packages/**"
  ]
}
